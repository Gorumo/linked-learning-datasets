        <rdfs:domain rdf:resource="&foaf;Organization"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/aiiso/schema#responsibilityOf -->

    <owl:ObjectProperty rdf:about="&aiiso;responsibilityOf">
        <rdfs:label xml:lang="en">responsibility of</rdfs:label>
