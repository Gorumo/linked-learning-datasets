        <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>
        <owl:inverseOf rdf:resource="&aiiso;responsibilityOf"/>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/spec/Agent"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
