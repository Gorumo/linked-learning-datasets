        <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/spec/Agent"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
