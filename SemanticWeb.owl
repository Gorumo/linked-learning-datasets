        <rdfs:label xml:lang="en">Module</rdfs:label>
        <rdfs:subClassOf rdf:resource="&aiiso;KnowledgeGrouping"/>
        <rdfs:comment rdf:datatype="&xsd;string">A Module is a KnowledgeGrouping that represents a cohesive collection of educational material referred to by the owning organization as a module.</rdfs:comment>
