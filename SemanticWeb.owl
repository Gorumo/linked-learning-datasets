        <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>
    </owl:Class>
    


    <!-- http://purl.org/vocab/aiiso/schema#Institution -->

    <owl:Class rdf:about="&aiiso;Institution">
        <rdfs:label xml:lang="en">Institution</rdfs:label>
        <rdfs:subClassOf rdf:resource="&foaf;Organization"/>
        <rdfs:comment rdf:datatype="&xsd;string">An Institution is the upper most level of an academic institution.</rdfs:comment>
        <dct:issued>2008-05-14</dct:issued>
