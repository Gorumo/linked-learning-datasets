    <owl:Ontology rdf:about="http://purl.org/vocab/aiiso/schema">
        <dc:contributor>Ian Corns</dc:contributor>
        <vann:preferredNamespacePrefix>aiiso</vann:preferredNamespacePrefix>
        <dc:creator>Rob Styles</dc:creator>
        <dc:identifier>http://purl.org/vocab/aiiso/schema-20080925</dc:identifier>
        <rdfs:comment xml:lang="en">
			The Academic Institution Internal Structure Ontology (AIISO) provides classes and properties to describe the internal organizational structure of an academic institution. AIISO is designed to work in partnership with Participation (http://purl.org/vocab/participation/schema), FOAF (http://xmlns.com/foaf/0.1/) and aiiso-roles (http://purl.org/vocab/aiiso-roles/schema) to describe the roles that people play within an institution.
    	</rdfs:comment>
        <dc:contributor>Sarah Bartlett</dc:contributor>
        <dc:date>2008-05-20</dc:date>
        <skos:historyNote></skos:historyNote>
        <dc:title xml:lang="en">Academic Institution Internal Structure Ontology (AIISO)</dc:title>
        <dc:rights>Copyright Â© 2008 Talis Information Ltd</dc:rights>
        <dc:contributor>Chris Wallace</dc:contributor>
        <dc:creator>Nadeem Shabir</dc:creator>
        <dct:issued>2008-05-14</dct:issued>
        <vann:preferredNamespaceUri>http://purl.org/vocab/aiiso/schema#</vann:preferredNamespaceUri>
        <cc:license rdf:resource="http://creativecommons.org/licenses/by/2.0/uk/"/>
        <dct:replaces rdf:resource="http://purl.org/vocab/aiiso/schema-20080520"/>
        <dct:hasFormat rdf:resource="http://purl.org/vocab/aiiso/schema-20080925.html"/>
        <dct:hasFormat rdf:resource="http://purl.org/vocab/aiiso/schema-20080925.rdf"/>
        <vann:usageNote rdf:resource="http://purl.org/vocab/aiiso/usagenote.html"/>
        <dct:isVersionOf rdf:resource="http://purl.org/vocab/aiiso/schema#"/>
    </owl:Ontology>
    


