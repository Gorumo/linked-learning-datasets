    <!-- http://purl.org/vocab/aiiso/schema#KnowledgeGrouping -->

    <owl:Class rdf:about="&aiiso;KnowledgeGrouping">
        <rdfs:label xml:lang="en">KnowledgeGrouping</rdfs:label>
        <owl:disjointWith rdf:resource="&foaf;Organization"/>
        <rdfs:comment rdf:datatype="&xsd;string">A KnowledgeGrouping represents a collection of resources, learning objectives, timetables, and other materials. The more specific subclasses of Subject, Programme, Course and Module are preferred.</rdfs:comment>
